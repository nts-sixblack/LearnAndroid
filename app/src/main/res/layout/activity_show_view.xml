<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ShowViewActivity">

    <TextView
            android:text="FullName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="142dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="56dp"/>
    <TextView
            android:text="UserName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:layout_marginTop="72dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="56dp"/>
    <TextView
            android:text="Password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:layout_marginTop="73dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="56dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/txtFullNameShowViewActitivity"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="144dp" android:layout_marginStart="40dp"
            app:layout_constraintStart_toEndOf="@+id/textView2"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/txtUserNameShowViewActivity"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="240dp" android:layout_marginStart="31dp"
            app:layout_constraintStart_toEndOf="@+id/textView3"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/txtPasswordShowViewActivity"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="323dp" android:layout_marginStart="34dp"
            app:layout_constraintStart_toEndOf="@+id/textView4"/>
</androidx.constraintlayout.widget.ConstraintLayout>